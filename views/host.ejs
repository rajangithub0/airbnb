    <script>
    const registerForm = document.getElementById('registerForm')

    const signupbtn = document.getElementById('signupbtn')

    const otpModal = new bootstrap.Modal(document.getElementById('otp-modal'))

    const signUpModal = new bootstrap.Modal(document.getElementById('sign-up-modal'))



    signupbtn.addEventListener('click', () => {
        if (!registerForm.checkValidity()) {
            return
        } else {
            signUpModal.hide()
            otpModal.show()
        }
    })

        const alert = document.querySelector(".alert")
        setTimeout(() => {
            alert.style.display = 'none'
        }, 4000)

        // get the range input element
            const rangeInput = document.querySelector('input[type="range"]');

            // add an event listener for the range input's "input" event
            rangeInput.addEventListener('input', function () {
                // prevent the default behavior of the scroll event
                event.preventDefault();
            });

        let slider = document.getElementById("myRange");
        let output = document.getElementById("demo");
        let price = document.getElementById("price")
        let pill = document.getElementById("pill")
        let totalNight = document.getElementById("total-night")
        output.innerHTML = slider.value;

        slider.oninput = function () {
            pill.innerHTML = `${this.value} Night`
            totalNight.innerHTML = `${this.value} Nights`
            output.innerHTML = this.value;
            const total = 4906 * this.value
            price.innerHTML = `₹${total.toLocaleString('en-In')}`

        }

        
    </script>
