            <script>

                function updateDateDifferance() {
                    let datefrom = new Date(document.getElementById('datefrom').value)
                    let dateto = new Date(document.getElementById('dateto').value)

                    let diffInMs = dateto.getTime() - datefrom.getTime()

                    let diffInDays = Math.floor(diffInMs / (1000 * 60 * 60 * 24))

                    if (diffInDays <= 0) {
                        document.getElementById('dateerror').innerHTML = 'check-out date must be after check-in'
                        const reservebtn = document.getElementById('reservebtn')
                        reservebtn.disabled = true
                    } else {
                        document.getElementById('dateerror').innerHTML = ''
                        const reservebtn = document.getElementById('reservebtn')
                        reservebtn.disabled = false
                    }
                    let period = document.getElementById('period')
                    period.innerHTML = diffInDays
                    const totalnightelement = document.getElementById('totalnight')
                    const selectguest = document.getElementById('selectguest')
                    const selectedOption = selectguest.options[selectguest.selectedIndex].value

                    const totalnight = document.getElementById('totalnight').innerText
                    const propertyprice = document.getElementById('propertyprice').innerText

                    const totalpricewithouttax = propertyprice * diffInDays
                    totalnightelement.innerHTML = totalpricewithouttax * selectedOption
                    const servicefees = document.getElementById('servicefees')


                    servicefees.innerHTML = totalpricewithouttax * selectedOption * 0.18

                    const totalwithgst = document.getElementById('totalwithgst')

                    totalwithgst.innerHTML = (totalpricewithouttax * selectedOption * 0.18) + (totalpricewithouttax * selectedOption)

                    // const totalPayment = (totalpricewithouttax * selectedOption * 0.18) + (totalpricewithouttax * selectedOption)

                    // const paymentdata = document.getElementById('paymentdata')

                    // paymentdata.setAttribute('data-amount',totalPayment)



                }




                function setDay1() {
                    const today = new Date();
                    const year = today.getFullYear();
                    const month = (today.getMonth() + 1).toString().padStart(2, '0');
                    const day = today.getDate().toString().padStart(2, '0');
                    const dateString = `${year}-${month}-${day}`; const datefrom = document.getElementById('datefrom');
                    datefrom.value = dateString;
                } setDay1()

                function setDay5() {
                    const today = new Date();
                    const fiveDaysFromNow = new Date(today.getTime() + (5 * 24 * 60 * 60 * 1000));
                    const year = fiveDaysFromNow.getFullYear();
                    const month = (fiveDaysFromNow.getMonth() + 1).toString().padStart(2, '0');
                    const day = fiveDaysFromNow.getDate().toString().padStart(2, '0');
                    const dateString = `${year}-${month}-${day}`;
                    const dateto = document.getElementById('dateto');
                    dateto.value = dateString;
                } setDay5()

                const btn = document.getElementById('btn')
                btn.addEventListener('click', () => {
                    btn.style.color = "#717171";
                    btn.style.borderColor = "rgba(128, 128, 128, 0.3)";
                    btn.classList.toggle("btn-active")
                })

                const dropdown = document.querySelector('.dropdown');
                document.addEventListener('click', (event) => {
                    if (!dropdown.contains(event.target)) {
                        btn.classList.remove("btn-active")
                    }
                });

 
            </script>

