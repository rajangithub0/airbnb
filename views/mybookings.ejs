<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto|Varela+Round">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/style.css">
    <link rel="stylesheet" href="/css/mybookingscss">

</head>

<body>
    <%- include('./partials/header')%>
        <div class="container-fluid p-0">
            <div class="table-wrapper mt-3" style="background: #fff;
  padding: 20px 25px;
  margin: 30px 0;
  border-radius: 3px;
  box-shadow: 0 1px 1px rgba(0, 0, 0, 0.05);">
                <div class="table-title" style="padding-bottom: 15px;
  background: #435d7d !important;
  color: #fff !important;
  padding: 16px 30px;
  margin: -20px -25px 0px !important;
  border-radius: 3px 3px 0 0;">
                    <div class="row">
                        <div class="col-sm-6">
                            <a href="#" style="color:white;text-decoration: none;">
                                <h2 class="d-none d-sm-block">My <b>Bookings</b></h2>
                            </a>
                        </div>
                        <!-- <div class="col-sm-6 d-flex justify-content-end btn-groups">
                        <a href="/admin-logout" class="btn btn-danger"> <span>Log out</span></a>
                        <a class="btn btn-success" data-bs-toggle="modal" data-bs-target=".list-property"> <span>Add New
                                Property</span></a>
                    </div> -->
                    </div>
                </div>
                <% if(message.success){%>
                    <div data-aos="fade-down" class="alert alert-success alert-dismissible fade show mt-3 mb-3"
                        role="alert" style="margin:0px 35px">
                        <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                            <symbol id="check-circle-fill" fill="currentColor" viewBox="0 0 16 16">
                                <path
                                    d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                            </symbol>
                            <symbol id="info-fill" fill="currentColor" viewBox="0 0 16 16">
                                <path
                                    d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z" />
                            </symbol>
                            <symbol id="exclamation-triangle-fill" fill="currentColor" viewBox="0 0 16 16">
                                <path
                                    d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
                            </symbol>
                        </svg>
                        <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img" aria-label="Success:">
                            <use xlink:href="#check-circle-fill" />
                        </svg>
                        <%= message.success%>
                            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                    </div>
                    <% } %>
                        <% if(message.error){%>
                            <div data-aos="fade-down" class="alert alert-danger alert-dismissible fade show mt-3 mb-3"
                                role="alert" style="margin:0px 35px">
                                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                                    <symbol id="check-circle-fill" fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                            d="M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zm-3.97-3.03a.75.75 0 0 0-1.08.022L7.477 9.417 5.384 7.323a.75.75 0 0 0-1.06 1.06L6.97 11.03a.75.75 0 0 0 1.079-.02l3.992-4.99a.75.75 0 0 0-.01-1.05z" />
                                    </symbol>
                                    <symbol id="info-fill" fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                            d="M8 16A8 8 0 1 0 8 0a8 8 0 0 0 0 16zm.93-9.412-1 4.705c-.07.34.029.533.304.533.194 0 .487-.07.686-.246l-.088.416c-.287.346-.92.598-1.465.598-.703 0-1.002-.422-.808-1.319l.738-3.468c.064-.293.006-.399-.287-.47l-.451-.081.082-.381 2.29-.287zM8 5.5a1 1 0 1 1 0-2 1 1 0 0 1 0 2z" />
                                    </symbol>
                                    <symbol id="exclamation-triangle-fill" fill="currentColor" viewBox="0 0 16 16">
                                        <path
                                            d="M8.982 1.566a1.13 1.13 0 0 0-1.96 0L.165 13.233c-.457.778.091 1.767.98 1.767h13.713c.889 0 1.438-.99.98-1.767L8.982 1.566zM8 5c.535 0 .954.462.9.995l-.35 3.507a.552.552 0 0 1-1.1 0L7.1 5.995A.905.905 0 0 1 8 5zm.002 6a1 1 0 1 1 0 2 1 1 0 0 1 0-2z" />
                                    </symbol>
                                </svg>
                                <svg class="bi flex-shrink-0 me-2" width="24" height="24" role="img"
                                    aria-label="Danger:">
                                    <use xlink:href="#exclamation-triangle-fill" />
                                </svg>
                                <%= message.error%>
                                    <button type="button" class="btn-close" data-bs-dismiss="alert"
                                        aria-label="Close"></button>
                            </div>
                            <% } %>
                                <table class="table table-striped table-hover">
                                    <thead class="bg-danger text-white">
                                        <tr>
                                            <th>Image</th>
                                            <th>Name</th>
                                            <th>Price</th>
                                            <th>Period</th>
                                            <th>Actions</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% mybookings.forEach(bookings => { %>
                                         
                                            <tr>
                                                <td>
                                                    <img src="/uploads/<%=bookings.image%>"
                                                    alt="" style="height: 50px;">
                                                </td>
                                                <td>
                                                 <%= bookings.propertyName %>
                                                </td>
                                                <td>
                                                    <%= bookings.totalPrice %>
                                                </td>
                                                <td>
                                                    <%- bookings.fromDate %> to <%= bookings.toDate %>
                                                </td>
                                                <td>
                                                    <!-- <a href="#" class="delete d-none d-lg-block d-sm-none"
                                                    style="background-image: radial-gradient( circle at center,#ff385c 0%,#e61e4d 27.5%,#e31c5f 40%,#d70466 57.5%,#bd1e59 75%,#bd1e59 100% );color:white;border-radius: 8px;padding: 10px 24px;font-weight: bolder;width: 165px;text-decoration: none;">Cancel
                                                    booking
                                                </a> -->
                                              


                                                <a href="/cancel/<%=bookings._id%>" class="delete d-block d-sm-block d-lg-none"
                                                style="background-image: radial-gradient( circle at center,#ff385c 0%,#e61e4d 27.5%,#e31c5f 40%,#d70466 57.5%,#bd1e59 75%,#bd1e59 100% );color:white;border-radius: 8px;padding: 10px 24px;font-weight: bolder; width: 100%;text-decoration: none;">Cancel
                                            </a>     
                                        </td>
                                    </tr>
                                    
                                    <% }) %>
                                    </tbody>
                                </table>
                                <div class="clearfix">
                                    <div class="hint-text">Showing <b>
                                            2
                                        </b> out of <b>
                                            2
                                        </b> entries</div>
                                    <ul class="pagination">
                                        <li class="page-item disabled"><a href="#">Previous</a></li>
                                        <li class="page-item active"><a href="#" class="page-link">1</a></li>
                                        <li class="page-item"><a href="#" class="page-link">2</a></li>
                                        <li class="page-item"><a href="#" class="page-link">3</a></li>
                                        <li class="page-item"><a href="#" class="page-link">4</a></li>
                                        <li class="page-item"><a href="#" class="page-link">5</a></li>
                                        <li class="page-item"><a href="#" class="page-link">Next</a></li>
                                    </ul>
                                </div>
            </div>
        </div>
        <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
        <script>
            AOS.init();
        </script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous">
            </script>
        <script>
            const btn = document.getElementById('btn')
            btn.addEventListener('click', () => {
                btn.style.color = "#717171";
                btn.style.borderColor = "rgba(128, 128, 128, 0.3)";
                btn.classList.toggle("btn-active")
            })

            const dropdown = document.querySelector('.dropdown');
            document.addEventListener('click', (event) => {
                if (!dropdown.contains(event.target)) {
                    btn.classList.remove("btn-active")
                }
            });


            const alert = document.querySelector(".alert")
            setTimeout(() => {
                alert.style.display = 'none'
            }, 4000)


            const uploadBtn = document.getElementById('uploadBtn');
            const uploadFileinput = document.getElementById('uploadFileinput');

            uploadBtn.addEventListener('change', function () {
                const fileName = this.value.split("\\").pop();
                uploadFileinput.value = fileName;
            });




        </script>
</body>

</html>